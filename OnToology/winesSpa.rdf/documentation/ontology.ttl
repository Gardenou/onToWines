#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/license
<http://purl.org/dc/elements/1.1/license> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionIRI
owl:versionIRI rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#IsFromRegion
:IsFromRegion rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Wine ;
              rdfs:range :Region ;
              rdfs:comment "DEfines the region where the wine comes from; can also define the region that refers to the DO" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#agedInBarrelsMadeOf
:agedInBarrelsMadeOf rdf:type owl:ObjectProperty ;
                     rdfs:domain :Wine ;
                     rdfs:range :Barrel .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#elaboratedWith
:elaboratedWith rdf:type owl:ObjectProperty ;
                rdfs:domain :Wine ;
                rdfs:range :Grape ;
                rdfs:comment "A wine is elaborated with a mixture of grapes" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#hasDO
:hasDO rdf:type owl:ObjectProperty ;
       rdfs:domain :Wine ;
       rdfs:range :DO .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Winery ;
             rdfs:range :Region ;
             rdfs:comment "The place where the winery is located, phisically." .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#producedBy
:producedBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :produces ;
            rdfs:domain :Wine ;
            rdfs:range :Winery ;
            rdfs:comment "Inverse to produces, a wine is produced by a winery" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#produces
:produces rdf:type owl:ObjectProperty ;
          rdfs:domain :Winery ;
          rdfs:range :Wine ;
          rdfs:comment "A winery produces a wine" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#alocholDegree
:alocholDegree rdf:type owl:DatatypeProperty ;
               rdfs:domain :Wine ;
               rdfs:range xsd:float ;
               rdfs:comment "The alocholic degree of a drink" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#barrelAgingTime
:barrelAgingTime rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Wine ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "The time the wine has been aging in a barrel(in months)" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#bottleAgingTime
:bottleAgingTime rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Wine ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "The time the wine has been aged in the bottle (in months)" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#hasBody
:hasBody rdf:type owl:DatatypeProperty ;
         rdfs:domain :Wine ;
         rdfs:range [ rdf:type rdfs:Datatype ;
                      owl:onDatatype xsd:integer ;
                      owl:withRestrictions ( [ xsd:minInclusive 0
                                             ]
                                             [ xsd:maxInclusive 5
                                             ]
                                           )
                    ] ;
         rdfs:comment "The body score, can only have a value from 1 to 5" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#hasNumberReviews
:hasNumberReviews rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Wine ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "The number of reviews an individual instance of wine has" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#hasPrice
:hasPrice rdf:type owl:DatatypeProperty ;
          rdfs:domain :Wine ;
          rdfs:range xsd:float ;
          rdfs:comment "The price of the individual instance of wine" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#hasReviewOf
:hasReviewOf rdf:type owl:DatatypeProperty ;
             rdfs:domain :Wine ;
             rdfs:range xsd:float ;
             rdfs:comment "The wine can have a review score, based on the consumer's experience" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#isFromYear
:isFromYear rdf:type owl:DatatypeProperty ;
            rdfs:domain :Wine ;
            rdfs:range xsd:gYear ;
            rdfs:comment "The year of the wine" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Aging
:Aging rdf:type owl:Class ;
       rdfs:comment "Depending on the kind of aging, a wine can be labelled" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Barrel
:Barrel rdf:type owl:Class .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Cava
:Cava rdf:type owl:Class ;
      rdfs:subClassOf :Type .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Crianza
:Crianza rdf:type owl:Class ;
         rdfs:subClassOf :Aging .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#DO
:DO rdf:type owl:Class ;
    rdfs:subClassOf <http://www.w3.org/2006/vcard/ns#Organization> ;
    rdfs:comment "DO serves as a regulation brand for the wine, assuring the costumer that it has a certain quality" ;
    rdfs:sameAs "https://dbpedia.org/page/Denominaci%C3%B3n_de_origen" ;
    rdfs:seeAlso "https://administracion.gob.es/pag_Home/Tu-espacio-europeo/derechos-obligaciones/empresas/inicio-gestion-cierre/derechos/denominaciones-origen.html"^^xsd:token .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#GranReserva
:GranReserva rdf:type owl:Class ;
             rdfs:subClassOf :Aging .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Grape
:Grape rdf:type owl:Class ;
       rdfs:comment "The kind of grape used to make the wine. There can be more than one kind involved in the making process" ;
       rdfs:seeAlso "https://en.wikipedia.org/wiki/List_of_grape_varieties"^^xsd:token .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Joven
:Joven rdf:type owl:Class ;
       rdfs:subClassOf :Aging .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#PedroXimenez
:PedroXimenez rdf:type owl:Class ;
              rdfs:subClassOf :Type .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Red
:Red rdf:type owl:Class ;
     rdfs:subClassOf :Wine ;
     owl:disjointWith :Rosé ,
                      :White .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#RedGrape
:RedGrape rdf:type owl:Class ;
          rdfs:subClassOf :Grape .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Region
:Region rdf:type owl:Class ;
        rdfs:comment "Spanish region where the wine comes from" .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Reserva
:Reserva rdf:type owl:Class ;
         rdfs:subClassOf :Aging .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#RiberaDelDueroRed
:RiberaDelDueroRed rdf:type owl:Class ;
                   rdfs:subClassOf :Type .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Rosé
:Rosé rdf:type owl:Class ;
      rdfs:subClassOf :Wine ;
      owl:disjointWith :White .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Sparkling
:Sparkling rdf:type owl:Class ;
           rdfs:subClassOf :Wine .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Sweet
:Sweet rdf:type owl:Class ;
       rdfs:subClassOf :Wine .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Tempranillo
:Tempranillo rdf:type owl:Class ;
             rdfs:subClassOf :Type .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#ToroRed
:ToroRed rdf:type owl:Class ;
         rdfs:subClassOf :Type .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Type
:Type rdf:type owl:Class .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Verdejo
:Verdejo rdf:type owl:Class ;
         rdfs:subClassOf :Type .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#White
:White rdf:type owl:Class ;
       rdfs:subClassOf :Wine .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#WhiteGrape
:WhiteGrape rdf:type owl:Class ;
            rdfs:subClassOf :Grape .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Wine
:Wine rdf:type owl:Class ;
      rdfs:comment "Alcoholic drink made from fermented grapes" ;
      rdfs:seeAlso "https://en.wikipedia.org/wiki/Wine"^^xsd:token .


###  http://www.semanticweb.org/denou/ontologies/2023/3/untitled-ontology-8#Winery
:Winery rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/2006/vcard/ns#Organization> ;
        rdfs:comment "The place where the wine is produced. Every winery must have license to sell the wine produced" .


###  http://www.w3.org/2006/vcard/ns#Organization
<http://www.w3.org/2006/vcard/ns#Organization> rdf:type owl:Class .


